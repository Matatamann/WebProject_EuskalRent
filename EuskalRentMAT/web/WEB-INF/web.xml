<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <!-- Declaracion -->
    <servlet>
        <servlet-name>logear</servlet-name>
        <servlet-class>Controlador.logear</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Controlador.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insertUser</servlet-name>
        <servlet-class>Controlador.insertUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateUser</servlet-name>
        <servlet-class>Controlador.updateUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>buscarAloj</servlet-name>
        <servlet-class>Controlador.buscarAloj</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroAloj</servlet-name>
        <servlet-class>Controlador.RegistroAloj</servlet-class>
    </servlet>
    <!-- FIN_declaracion -->
    <!-- Mapeado -->
    <servlet>
        <servlet-name>borrarReserva</servlet-name>
        <servlet-class>Controlador.borrarReservaArrendado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>borrarReservaArrendatario</servlet-name>
        <servlet-class>Controlador.borrarReservaArrendatario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>borrarReservaArrendador</servlet-name>
        <servlet-class>Controlador.borrarReservaArrendador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateAloj</servlet-name>
        <servlet-class>Controlador.updateAloj</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteUser</servlet-name>
        <servlet-class>Controlador.deleteUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registroReserva</servlet-name>
        <servlet-class>Controlador.registroReserva</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>borrarAlquiler</servlet-name>
        <servlet-class>Controlador.borrarAlquiler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateReserva</servlet-name>
        <servlet-class>Controlador.updateReserva</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addSaldo</servlet-name>
        <servlet-class>Controlador.addSaldo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>decodificadorImajen</servlet-name>
        <servlet-class>Controlador.decodificadorImajen</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logear</servlet-name>
        <url-pattern>/logear</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insertUser</servlet-name>
        <url-pattern>/insertUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateUser</servlet-name>
        <url-pattern>/updateUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>buscarAloj</servlet-name>
        <url-pattern>/buscarAloj</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroAloj</servlet-name>
        <url-pattern>/RegistroAloj</url-pattern>
    </servlet-mapping>
    <!-- FIN_Mapeado -->
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <!-- Base de datos -->
    <resource-ref>
        <description>Pool conexiones MySQL</description>
        <res-ref-name>jdbc/ConexionMySQL</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>
    <!-- FIN_Base de datos -->
    <servlet-mapping>
        <servlet-name>borrarReserva</servlet-name>
        <url-pattern>/borrarReserva</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>borrarReservaArrendatario</servlet-name>
        <url-pattern>/borrarReservaArrendatario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>borrarReservaArrendador</servlet-name>
        <url-pattern>/borrarReservaArrendador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateAloj</servlet-name>
        <url-pattern>/updateAloj</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteUser</servlet-name>
        <url-pattern>/deleteUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registroReserva</servlet-name>
        <url-pattern>/registroReserva</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>borrarAlquiler</servlet-name>
        <url-pattern>/borrarAlquiler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateReserva</servlet-name>
        <url-pattern>/updateReserva</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addSaldo</servlet-name>
        <url-pattern>/addSaldo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>decodificadorImajen</servlet-name>
        <url-pattern>/decodificadorImajen</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
